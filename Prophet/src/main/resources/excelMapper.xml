<workbook>
    <worksheet name="问题单表格">
        <section startRow="0" endRow="0" />
        <loop startRow="1" endRow="1" item="result" var="dtsInfo" varType="com.hackathon.prophet.pojo.SingleDtsBase">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">dtsInfo.id</mapping>
                <mapping row="1" col="2">dtsInfo.version</mapping>
                <mapping row="1" col="3">dtsInfo.id</mapping>
                <mapping row="1" col="4">dtsInfo.id</mapping>
                <mapping row="1" col="5">dtsInfo.id</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0" />
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>